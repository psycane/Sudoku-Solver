(function ($) {$.fn.watermark = function (options) {var defaults = {text: null,color: "#cccccc",fontSize: ".850em",top: "-5px",left: "1px",className: "watermark"};return this.filter('input:text').each(function () {defaults.text = $(this).attr("title");settings = $.extend({}, defaults, options);var wrapper$ = $("<span />", { "style": "position:relative;" });$(this).after(wrapper$).detach().appendTo(wrapper$);var watermark$ = $("<label />", { "text": settings.text, "for": $(this).attr("id"), "class": settings.className });watermark$.css({"position": "absolute","top": settings.top,"left": settings.left,"z-index": "1","color": settings.color,"font-size": settings.fontSize, "border": "0px"});watermark$.appendTo(wrapper$);$(this).focus(function () {if (this.value == "") { watermark$.hide(); }});$(this).blur(function () {if (this.value == '') { watermark$.show(); }});if (!$(this).val() == "") { watermark$.hide(); }});};})(jQuery);;



/* Comment Generated by Combres - Resource '/JS/plugins/jquery.bgPos.js' (Mode: Static) */
/*! Copyright (c) 2010 Brandon Aaron (http://brandonaaron.net)
 * Licensed under the MIT License (LICENSE.txt).
 */
(function($) {
    // backgroundPosition[X,Y] get hooks
    var $div = $('<div style="background-position: 3px 5px">');
    $.support.backgroundPosition   = $div.css('backgroundPosition')  === "3px 5px" ? true : false;
    $.support.backgroundPositionXY = $div.css('backgroundPositionX') === "3px" ? true : false;
    $div = null;

    var xy = ["X","Y"];

    // helper function to parse out the X and Y values from backgroundPosition
    function parseBgPos(bgPos) {
        var parts  = bgPos.split(/\s/),
            values = {
                "X": parts[0],
                "Y": parts[1]
            };
        return values;
    }

    if (!$.support.backgroundPosition && $.support.backgroundPositionXY) {
        $.cssHooks.backgroundPosition = {
            get: function( elem, computed, extra ) {
                return $.map(xy, function( l, i ) {
                    return $.css(elem, "backgroundPosition" + l);
                }).join(" ");
            },
            set: function( elem, value ) {
                $.each(xy, function( i, l ) {
                    var values = parseBgPos(value);
                    elem.style[ "backgroundPosition" + l ] = values[ l ];
                });
            }
        };
    }

    if ($.support.backgroundPosition && !$.support.backgroundPositionXY) {
        $.each(xy, function( i, l ) {
            $.cssHooks[ "backgroundPosition" + l ] = {
                get: function( elem, computed, extra ) {
                    var values = parseBgPos( $.css(elem, "backgroundPosition") );
                    return values[ l ];
                },
                set: function( elem, value ) {
                    var values = parseBgPos( $.css(elem, "backgroundPosition") ),
                        isX = l === "X";
                    elem.style.backgroundPosition = (isX ? value : values[ "X" ]) + " " + 
                                                    (isX ? values[ "Y" ] : value);
                }
            };
            $.fx.step[ "backgroundPosition" + l ] = function( fx ) {
                $.cssHooks[ "backgroundPosition" + l ].set( fx.elem, fx.now + fx.unit );
            };
        });
    }
})(jQuery);
;



/* Comment Generated by Combres - Resource '/JS/jquery.spie.globalHeader.js' (Mode: Static) */
var SearchCallbackCount = 0;    //Keeps tabs on how many asynchronous calls have been made to the search service.
var autoSuggestRunning = false;
//SOLR
//var solrSwitch = $("#hfIsSolrSearchEnabled").val();
//var isSolrSearchEnabled = true;
//isSolrSearchEnabled = solrSwitch == "1" ? true : false;

//var searchMaxAllowChars = $("#hfSolrMaxAllowSearchChar").val();
//var autoSuggestRunning = false;

$(
        function () {
            //Selectors for styling Navigation
            $('.topNav li:first-child').addClass('first');
            $('.topNav li:last-child').addClass('last');

            //Image replacement for SPIE Network logo in umbrella nav
            // $('#globalHeader_divUmbrellaNavigationBar a').filter(':first').addClass('sprite-globalHeader sprite-logo_network_small ir');
            $('#globalHeader_divUmbrellaNavigationBar a:contains("Journals")').html('Journals &gt;');


            //Microsite nav bar active nav item: cross-browser sprite centering
            //Initially the background-position X value is set to -9999px in global.css
            //For this to work in IE<9 Jquery<1.7, plugin jquery.bgPos.js is required
            $('.microNavBar li.active').css('background-position', function (index, value) {
                var myPos = value;
                var myMiddle = (parseInt($(this).width()) / 2) - 4;
                var newPos = myPos.replace('-9999px', myMiddle + 'px');
                return newPos;
            });


            //SOLR
            var solrSwitch = $("#hfIsSolrSearchEnabled").val();
            var isSolrSearchEnabled = true;
            isSolrSearchEnabled = solrSwitch == "1" ? true : false;
            
            //Adding the click event handler to the search button.
            $('.searchButton').click
            (
                function () {
                    var queryText = $("#txtSearchTerm").val();

                    //var searchScope = $("#ddlGroup").val();
                    var searchScope = ($("#ddlGroup").length > 0) ? $("#ddlGroup").val() : "";

                    if (isSolrSearchEnabled) {
                        //SOLR Search is ON.
                        if (!autoSuggestRunning) {
                            doSolrSearch(queryText);    
                        }
                    } else {
                        if (SearchCallbackCount != 0)
                            return false;

                        //Performing the search.
                        //doSearch(queryText, searchScope, 1);
                        doGlobalSearch(queryText);
                    }

                }
            );

            // umbrella site search
            $('.searchButton3').click
            (
                function () {
                    var queryText = $("#divtextSearch2").children("input[type=text]").val();

                    if (isSolrSearchEnabled) {
                        //SOLR Search is ON.

                        if (!autoSuggestRunning) {
                            doSolrSearch(queryText);
                        }

                    } else {

                        if (SearchCallbackCount != 0)
                            return false;

                        doGlobalSearch(queryText);
                    }
                }
            );

            /*Search Fix Ends*/
            return false;
        }
    );

        $(document).ready
    (
        function () {
            function stopRKey(evt) {
                var evt = (evt) ? evt : ((event) ? event : null);
                var node = (evt.target) ? evt.target : ((evt.srcElement) ? evt.srcElement : null);

                if ((evt.keyCode == 13) && (node.type == "text")) { return false; }
            }

            document.onkeypress = stopRKey;

            //SOLR
            var solrSwitch = $("#hfIsSolrSearchEnabled").val();
            var isSolrSearchEnabled = true;
            isSolrSearchEnabled = solrSwitch == "1" ? true : false;
            
            //Adding the click event handler to the search button.
            $("#txtSearchTerm").keydown(function (e) {
                if (e.keyCode == 13) {
                    var queryText = $("#txtSearchTerm").val();

                    if (isSolrSearchEnabled) {
                        //SOLR Search is ON
                        if (!autoSuggestRunning) {
                            doSolrSearch(queryText);
                        }
                    } else {
                        if (SearchCallbackCount != 0)
                            return false;

                        doGlobalSearch(queryText);
                    }
                }
            });

            // umbrella site search
            $("#divtextSearch2").children("input[type=text]").keydown(function (e) {
                if (e.keyCode == 13) {
                    var queryText = $("#divtextSearch2").children("input[type=text]").val();

                    if (isSolrSearchEnabled) {
                        //SOLR Search is ON
                        if (!autoSuggestRunning) {
                            doSolrSearch(queryText);
                        }
                    } else {
                        if (SearchCallbackCount != 0)
                            return false;

                        doGlobalSearch(queryText);
                    }
                }
            });

