$(document).ready(function () {

    if (!isMobile) {

        /***************************/
        /*     article rotator     */
        /***************************/
        $(".articleRotator").each(function () {
            $this = $(this);
            listL = $this.find('ul li').length;
            if (listL == 0) {
                $this.hide();
            } else {
                var gotoBTN = new Array();
                $this.find('ul li').each(function (i, v) {
                    $this.find(".gotoBTN").append('<span class="' + i + '"></span>');
                    gotoBTN.push(".gotoBTN ." + i);
                    if (i == listL - 1) {
                        $this.find(".gotoBTN span").eq(0).addClass("selected")
                        $this.find('.articleCarousel').jCarouselLite({
                            btnNext: ".b_carousel_next",
                            btnPrev: ".b_carousel_prev",
                            mouseWheel: true,
                            visible: 1,
                            btnGo: gotoBTN,
                            //                            circular: false,
                            //                            afterEnd: function (a) {
                            //                                $this.find(".gotoBTN span").removeClass("selected").eq($(a[0]).index()).addClass("selected");  
                            circular: true,
                            auto: 6000,
                            speed: 500,
                            afterEnd: function (a) {
                                var curIndex = $(a[0]).index();
                                if (curIndex == 0) curIndex = listL - 1;
                                else if (curIndex == listL + 1) curIndex = 0;
                                else curIndex = curIndex - 1;
                                $this.find(".gotoBTN span").removeClass("selected").eq(curIndex).addClass("selected");
                            }
                        });
                    }
                });
            }
        });
        /**/
//        var numberArticles = $('.articleRotator ul li').length; //check to see if there are any figures and how many

